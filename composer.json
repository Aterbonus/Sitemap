{
	"name": "Aterbonus/sitemap",
	"type": "library",
	"description": "PHP XML sitemap generation for PHP ^5.6 forked from \"thepixeldeveloper/sitemap\"",
	"keywords": ["php", "sitemap", "xml", "sitemap-php", "sitemap-builder", "sitemap-generator", "sitemap-xml", "xml-sitemap"],
	"license": "MIT",
	"authors": [
		{
			"name": "Gustasvo Alfredo Marín Sáez",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.6"
	},
	"require-dev": {
		"phpunit/phpunit": "^5"
	},
	"autoload": {
		"psr-4": {
			"Aterbonus\\Sitemap\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Aterbonus\\Sitemap\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit --coverage-text --colors=never --coverage-html ./coverage"
	}
}
